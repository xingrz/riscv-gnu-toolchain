# Simple version: imafdc + Zb
#MULTILIBGEN="rv32emc-ilp32e--;rv32emac-ilp32e--;rv32imc-ilp32--;rv32imac-ilp32--;rv32imafc-ilp32f--;rv32imafdc-ilp32d--;rv32imac_zba_zbb_zbs-ilp32--;rv32imafc_zba_zbb_zbs-ilp32f--;rv32imafdc_zba_zbb_zbs-ilp32d--;rv64imac-lp64--;rv64imafc-lp64f--;rv64imafdc-lp64d--;rv64imac_zba_zbb_zbs-lp64--;rv64imafc_zba_zbb_zbs-lp64f--;rv64imafdc_zba_zbb_zbs-lp64d--;"
# build toolchain with multilib or not, 0 or 1
# if MULTILIBGEN is empty, will do multilib by default
DOMULTILIB=1
# strip toolchain after build, 0 or 1
DOSTRIP=1
# build llvm or not, 0 or 1
DOLLVM=1
# build toolchain or not, 0 or 1
DOBUILD=1
# archive toolchain or not, 0 or 1
DOARCHIVE=1
# build libncrt or not, 0 or 1
DOLIBNCRT=1
# libncrt build config, such riscv64-unknown-elf
LIBNCRTBLDCFG=riscv64-unknown-elf
# build doc or not, 0 or 1
DODOC=1
# clean build directory after successfully build, 0 or 1
DOCLEAN=1
# clean install prefix directory before toolchain build, 0 or 1
DOCLEANPREFIX=1
# default arch/abi/isaspec/tune configuration
# glibc need atomic extension, so default changed to rv64ima
DEFRVCFG="--with-arch=rv64ima --with-abi=lp64"
# default build jobs
JOBS=16
